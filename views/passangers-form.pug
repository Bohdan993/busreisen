  const data = passangersInfo?.map(el => {
    const prefix = el[0].split("-")?.[0];
    const result = [];
    for(const key in el[1]) {
      if(regexArr.some(rgx => rgx.test(key))) {
        const obj = {[prefix + "-" + key]: el[1][key]};
        result.push(obj);
      }
    }
    return result;
  });

